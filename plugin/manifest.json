{
    "manifest_version": 2,
    "name": "HateBlockPlus",
    "version": "1.0",
    "description": "Uses machine learning to hide hateful tweets.",
    "permissions": ["https://twitter.com/*"],
    "content_scripts": 
    [
        {
            "matches": ["https://twitter.com/*"],
            "js": ["basic_plugin.js"]
        }
    ]
}